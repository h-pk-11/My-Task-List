{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "consistent-return":"off",
        "no-plusplus": "off",
        "func-names": ["error", "never"],
        "no-param-reassign": "off",
        "linebreak-style": ["error", "windows"],
        "camelcase": "off",
        "indent": "error",
        "no-underscore-dangle": "off",
        "no-use-before-define": ["error", { "functions": false }]
    }
}
